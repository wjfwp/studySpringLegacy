<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- 컬렉션을 주입하는 방법 -->
	<bean id="dev" class="ex03.DatabaseDev">
		<property name="url" value="jdbc:mysql://location.~~~"/>
		<property name="uid" value="spring"/>
		<property name="upw" value="1234"/>
	</bean>
	
	<bean id="customer" class="ex04.Customer">
		<property name="lists">
			<list>
				<value>문자열</value>
				<value>홍길동</value>
				<value>이순신</value>
			</list>
		</property>
		
		<property name="dao">
			<list>
				<bean class="ex03.MemberDAO">
					<property name="dataSource" ref="dev"/>
				</bean>
			</list>
		</property>
		
		<property name="map">
			<map>
				<entry key="xxx" value="홍길동"/>
				<entry key="aaa123">
					<bean class="ex03.MemberDAO">
						<property name="dataSource" ref="dev"/>
					</bean>
				</entry>
			</map>
		</property>
		
	</bean>

</beans>
